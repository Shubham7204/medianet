# Example Environment Variables for MediaNet API
# Copy this file to .env and fill in your actual values

# Exa API Configuration - Get from https://exa.ai/
EXA_API_KEY=your_exa_api_key_here

# Google Gemini API Configuration - Get from https://aistudio.google.com/
GEMINI_API_KEY=your_gemini_api_key_here
# Alternative name for Gemini API key
GOOGLE_API_KEY=your_gemini_api_key_here

# Pinecone API Configuration (for RAG engine) - Get from https://www.pinecone.io/
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_INDEX_NAME=pdf-query-index

# Optional: Gemini Model Configuration
GEMINI_MODEL=gemini-2.5-pro

# Optional: Other configuration
# DEBUG=True
# LOG_LEVEL=INFO